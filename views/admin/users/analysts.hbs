<h4>Lista de Analistas</h4>
<p></p>
<div class="table-responsive">
  <table id="tb-user" class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Nome</th>
        <th>Telefone</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      {{#each users}}
        <tr>
          <th>{{fullname}}</th>
          <th>{{cellphone}}</th>
          <th>{{email}}</th>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>