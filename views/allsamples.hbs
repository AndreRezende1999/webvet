<form action="/allsamples" method="post">
  <div class="container-fluid">
    <div class="row">

      <div class="container-fluid col-sm-12 text-center">
        <h4>Mapas de Trabalho</h4>
      </div>
      <div class="container-fluid col-sm-12 p-3"></div>

      <div class="container-fluid text-center">
        <div class="templateprint row col-sm-12 m-0">
          {{#each names}}

            <table class="table-stripped col-sm-2" name="table-afla">

              <thead>
                <tr>
                  <th>{{this}}</th>
                  <th>Absorv√¢ncia</th>
                </tr>
              </thead>

              <tbody>
                {{#for 1 6 1}}
                  <tr>
                    <th>P{{this}}</th>
                    <th>
                      <div class="input-group-sm">
                        <input name="{{../this}}Calibrator[P{{this}}]" class="form-control">
                      </div>
                    </th>
                  </tr>
                {{/for}}
            </table>
          {{/each}}
        </div>
      </div>

      <div class="container-fluid col-sm-12 p-3"></div>

      <div class="btn-group-justified col-sm-12 container-fluid">
        <div class="btn btn-back">
          <a href="/queue" class="btn btn-success" id="test">Voltar</a>
        </div>
        <div class="btn btn-abs">
          <button class="btn btn-success">Finalizar</button>
        </div>
      </div>

    </div>
  </div>

</form>


